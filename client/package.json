    "nouislider": "14.6.3",
    "react": "17.0.1",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.1",
    "react-facebook-login": "^4.1.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "reactstrap": "8.9.0"
  },
  "devDependencies": {
    "gulp": "4.0.2",
